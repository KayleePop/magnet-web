{
  "name": "magnet-web",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "bundle": "browserify --plugin tinyify index.js > bundle.js",
    "generate-worker": "echo \"(path) => path.startsWith('/magnet-web/magnet/')\" | stream-to-sw > worker.js",
    "build": "npm run bundle && npm run generate-worker"
  },
  "author": "KayleePop",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.5.1",
    "standard": "^14.3.3",
    "tinyify": "^2.5.2"
  },
  "dependencies": {
    "mime": "^3.0.0",
    "range-parser": "^1.2.1",
    "stream-to-sw": "^1.1.0",
    "webtorrent": "^0.108.0"
  }
}
